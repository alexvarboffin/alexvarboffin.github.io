Windows Registry Editor Version 5.00

; Context menu Extension for png, jpg
; Google Play image generator, just install imagemagick.exe 

[HKEY_CLASSES_ROOT\*\shell\Custom Menu]
"MUIVerb"="## CONVERTER ##"
"SubCommands"="ROTATE_90;1280x720;180x120;Header;pixel_1440x2880;pixel_2880x1440;Google_Play_1024x500;GP_512x512;Google_Play_180x120;android-bg-480x800;kawork-660x440;256x256_JPG;42x42px"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\killserver]
"Icon"="C:\\Windows\\System32\\calc.exe"
"Position"="Middle"
@="ADB kill-server"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\killserver\Command]
@="\"C:\\android\\sdk\\platform-tools\\adb.exe\" kill-server"


[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\zzzzzz]
"Icon"="C:\\Windows\\System32\\calc.exe"
"Position"="Middle"
@="ADB_INSTALL"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\zzzzzz\Command]
@="\"C:\\android\\sdk\\platform-tools\\adb.exe\" install -r \"%1\""



[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\nox_adb_install]
"Icon"="C:\\Program Files\\Vega\\Vega.exe"
"Position"="Middle"
@="$NOX adb install"

[HKEY_LOCAL_MACHINE\SOFTWARE\Classes\*\shell\nox_adb_install\Command]
@="\"D:\\Program Files\\Nox\\bin\\nox_adb.exe -s 127.0.0.1:62001\" install -r \"%1\""[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ROTATE_90]
@="ROTATE_90"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\ROTATE_90\command]
@="cmd /c convert -background none -rotate \"90\" \"%1\" %random%.png"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\1280X720]
@="1280x720"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\1280x720\command]
@="cmd /c convert \"%1\" -resize 1280x720! 1280x720_tv_banner_\"%~n1\".png"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\180X120]
@="180x120"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\180x120\command]
@="cmd /c convert \"%1\" -resize 180x120! 180x120_icon_\"%~n1\".png"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\HEADER]
@="Header"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\Header\command]
@="cmd /c convert \"%1\" -resize 504x288 nav_header.png"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\PIXEL_1440X2880]
