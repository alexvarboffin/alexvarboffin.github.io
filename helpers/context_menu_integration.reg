[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\PIXEL_2880X1440]
@="pixel_2880x1440"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\pixel_2880x1440\command]
@="cmd /c convert \"%1\" -resize 2880x1440! pixel_2880x1440-%%RANDOM%%.png"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\GOOGLE_PLAY_1024X500]
@="Google_Play_1024x500"
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\Google_Play_1024x500\command]
@="cmd /c convert \"%1\" -resize 1024x500! Google_Play_1024x500-%%RANDOM%%.png"
